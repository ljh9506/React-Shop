{
  "name": "mern-shop",
  "version": "1.0.0",
  "description": "MERN ShoppingMall",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "nodemon backend/server",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "client2": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "dev2": "concurrently \"npm run server\" \"npm run client2\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d"
  },
  "author": "LeeJoonHee",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.887.0",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.8",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6"
  }
}
